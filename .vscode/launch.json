    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Deno Program with pnpm",
            "type": "node",
            "program": "${workspaceFolder}/main.ts", 
            "cwd": "${workspaceFolder}",
            "env": {},
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "attachSimplePort": 9229
        }
